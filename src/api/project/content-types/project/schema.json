      "type": "string",
      "required": true
    },
    "services": {
      "type": "json",
      "required": true
    },
    "duration": {
      "type": "string",
      "required": true
    },
    "tools": {
      "type": "json",
      "required": true
    },
    "team_size": {
      "type": "string",
      "required": true
    },
